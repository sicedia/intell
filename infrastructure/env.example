# =============================================================================
# Infrastructure Environment Variables for Production
# Copy to .env and fill in your values
# =============================================================================

# PostgreSQL Database
# -------------------
POSTGRES_DB=intell
POSTGRES_USER=intell_user
POSTGRES_PASSWORD=change-this-strong-password-in-production

# Frontend URLs (used during Docker build)
# ----------------------------------------
# These are baked into the frontend during build
# For internal Docker network communication, use service names
# For external access, use your domain
NEXT_PUBLIC_API_BASE_URL=https://your-domain.com/api
NEXT_PUBLIC_WS_BASE_URL=wss://your-domain.com/ws
